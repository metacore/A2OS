		PROCEDURE ^ AllocationHint*(physicalRegisterNumber: LONGINT); 
		PROCEDURE ^ Mapped*(physicalRegisterNumber: LONGINT): Ticket; 
		PROCEDURE ^ Dump*(w: Streams.Writer); 
